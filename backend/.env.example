# Environment configuration for the backend
# Copy this file to .env and fill in your API keys/values.

# OpenAI API key for generating analysis
OPENAI_API_KEY=

# Finnhub API key for market data
FINNHUB_API_KEY=

# Database connection string
# Defaults to SQLite when not set: sqlite:///./users.db
DATABASE_URL=

# Path to the Oracle wallet directory for Autonomous DB connections
ORACLE_WALLET_PATH=

# Secret key for signing JWT tokens
# Defaults to "change-me" when not set
SECRET_KEY=
